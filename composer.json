{
  "name": "ronnel/bonza-quotes",
  "description": "Bonza Quotes - A simple quote management WordPress plugin",
  "type": "wordpress-plugin",
  "version": "1.0.0",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "BonzaQuotes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BonzaQuotes\\Tests\\": "tests/"
    }
  },
"config": {
    "vendor-dir": "vendor",
    "preferred-install": "dist"
  },
  "scripts": {
    "post-autoload-dump": [
      "echo Composer dump completed."
    ]
  }
}